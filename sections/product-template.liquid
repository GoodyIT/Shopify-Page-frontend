{%- assign current_variant = product.selected_or_first_available_variant -%}
{%- assign current_variant_sale = false -%}
{% if current_variant.compare_at_price > current_variant.price %}
  {%- assign current_variant_sale = true -%}
{% endif %}

<div class="product-template" data-section-id="{{ section.id }}" data-section-type="product-template" data-variant-id="{{ current_variant.id }}" itemscope itemtype="http://schema.org/Product">

  <meta itemprop="name" content="{{ product.title }}">
  <meta itemprop="url" content="{{ shop.url }}{{ product.url }}">
  <meta itemprop="image" content="{{ product.featured_image.src | img_url: 'grande' }}">

  {% comment %}
  ------------------------------------------------------------------------------
  Product Featured Image
  ------------------------------------------------------------------------------
  {% endcomment %}
  <div class="pt-60" style="position: relative;">
    {{ 'side_flowers_1_home.png' | asset_url | img_tag: "alt", "product_left_side small--hide" }}
    {{ 'side_flowers_6_our_story.png' | asset_url | img_tag: "alt", "product_right_side small--hide" }}
    <div class="page-width">
      <div class="grid grid--uniform">
        <div class="grid__item small--one-whole medium-up--one-half">
          {% assign featured_image = current_variant.featured_image | default: product.featured_image %}
          {% for image in product.images %}
           
              {% assign max_height = 650 %}
              {% assign max_width = 980 %}
              {% assign max_aspect_ratio = 1.50769 %}

              {% comment %}
              We need to figure out the max width we want the image to be on the page
              based on the aspect ratio of the image and based on the size of the
              image.
              {% endcomment %}
              {% if image.aspect_ratio < max_aspect_ratio%}
                {% if image.height < max_height %}
                  {% assign max_width = image.width | times: image.aspect_ratio | round %}
                {% else %}
                  {% assign max_width = max_height | times: image.aspect_ratio | round %}
                {% endif %}
              {% else %}
                {% if image.width < max_width %}
                  {% assign max_width = image.width %}
                {% endif %}
              {% endif %}

              {% comment %}
              Create a container for the image that will never go wider than the
              max width we determined above, and that will keep the same aspect ratio
              of the image by using 'padding-top'.
              {% endcomment %}
              <div class="product__image-container" style="max-width: {{ max_width }}px" data-id="{{ image.id }}" data-variant-image-toggle-hide>
                <a class="product__image-wrapper product-slideshow__open" >

                  {% comment %}
                  Only give the visible variant image a src so that it shows an image
                  as quickly as possible. Let lazysizes.js load in the other images when
                  they become visible.
                  {% endcomment %}
                  {% assign img_url = image | img_url: '1x1' | replace: '_1x1.', '_{width}x.' %}
                  <img class="lazyload"
                      style="min-width: 100%;" 
                      src="{{ img_url }}"
                      data-src="{{ img_url }}"
                      data-widths="[360, 540, 720, 900, 1080, 1296, 1512, 1728, 2048]"
                      data-aspectratio="{{ image.aspect_ratio }}"
                      data-sizes="auto"
                      alt="{{ image.alt | escape }}"
                      data-variant-image>
                </a>
              </div>
              {% break %}
          {% endfor %}
        </div>
        <div class="grid__item small--one-whole medium-up--one-half">
          {% comment %}
          ------------------------------------------------------------------------------
          Product Form & Description
          ------------------------------------------------------------------------------
          {% endcomment %}
          <div class="medium-up--ten-twelfths product__content-header" style="padding-top: 0">
            <div class="">
              {% comment %}
              {% if section.settings.show_vendor %}
                <span class="product__vendor text-small text-center" itemprop="brand">{{ product.vendor }}</span>
              {% endif %}
              {% endcomment %}
              <h1 class="product__title h3 mb-3" itemprop="name">{{ product.title }}</h1>
              <p class="product__price mb-20 {% if current_variant_sale %} product__price--sale{% endif %}" data-product-price aria-live="polite">

                <span class="product__sale-price-label visually-hidden">{{ 'products.product.sale_price' | t }}</span>
                <span class="product__regular-price-label visually-hidden">{{ 'products.product.price' | t }}</span>
                <span class="product__current-price" data-regular-price>{{ current_variant.price | money }}</span>

                <span class="product__compare-price-label visually-hidden">{{ 'products.product.regular_price' | t }}</span>
                <s class="product__compare-price" data-compare-price>{{ current_variant.compare_at_price | money }}</s>

                {% include 'product-unit-price', variant: current_variant %}
              </p>
              {% if product.title == 'Stronger Immunity Tea Box' %}
                {% assign shop_on_amazon_line = "https://www.amazon.com/dp/B07K7V3RF4/" %}
              {% elsif product.title == 'Tummy Tamer Tea Box' %}
                {% assign shop_on_amazon_line = "https://www.amazon.com/dp/B07RTNRZP1/" %}
              {% elsif product.title == 'Night Time Herbal Tea Box' %}
                {% assign shop_on_amazon_line = "https://www.amazon.com/dp/B07K7VYQGC/" %}
              {% endif %}
              <a href="{{shop_on_amazon_line}}" class="custom_btn shop_now_btn">
                Shop on amazon
              </a>

              <ul class="product__description1">
                <li>40+ servings of teas (10 blends, 4 tea bags of each) and 10 counts of pure clover honey sticks.</li>
                <li>Free shipping</li>
                <li>Perfect for gifting. Not a subscription. </li>
                <li>Dimension: 7.5 x 7.5 x 2.2 inches</li>
                <li>Weight: 12 oz</li>
              </ul>

              {%- if shop.taxes_included or shop.shipping_policy.body != blank -%}
                <div class="product__policies rte">
                  {%- if shop.taxes_included -%}
                    {{ 'products.product.include_taxes' | t }}
                  {%- endif -%}
                  {%- if shop.shipping_policy.body != blank -%}
                    {{ 'products.product.shipping_policy_html' | t: link: shop.shipping_policy.url }}
                  {%- endif -%}
                </div>
              {%- endif -%}

            </div>

            <div class="product__content-main">
              <div class="product__description rte" itemprop="description" class="rte">
                {{ product.description }}
              </div>

              {% comment %}
              <div class="product__form-container" itemprop="offers" itemscope itemtype="http://schema.org/Offer">
                <div class="product__form-wrapper">
                  <meta itemprop="price" content="{{ current_variant.price | divided_by: 100.00 }}">
                  <meta itemprop="priceCurrency" content="{{ cart.currency.iso_code }}">
                  <link itemprop="availability" href="http://schema.org/{% if current_variant.available %}InStock{% else %}OutOfStock{% endif %}">

                  {% include 'product-form' %}

                  {% if section.settings.show_share_buttons %}
                    {% if settings.share_facebook or settings.share_twitter or settings.share_pinterest %}
                      <div class="product__share-wrapper small--hide">
                        <div class="product__share">
                            {% include 'social-sharing', type: "product", links: 'bottom' share_title: product.title, share_permalink: product.url, share_image: product %}
                        </div>
                      </div>
                    {% endif %}
                  {% endif %}
                </div>
              </div>
              {% endcomment %}
            </div>
            <p class="key__herbs">Key herbs</p>
            <div class="grid key-herb-block">
              {% assign herbs = "Green Tea Leaves, Lemon, Lemongrass, Raw Honey| Chamomile, Valerian Root, Lavender, Lemon Balm| Peppermint, Chamomile, Ginger Root, Fennel" | split: "| " %}
              {% assign idx = 0 %}
                {% for image in product.images %}
                  {% if forloop.index != 1 %}
                  {% unless image.attached_to_variant? and section.settings.hide_variant_images%}

                    <div class="grid__item small--one-quarter medium-up--one-quarter pr-1">
                      <a class="herbs__subimage-container product-slideshow__open" >
                        {% assign img_url = image | img_url: '1x1' | replace: '_1x1.', '_{width}x.' %}
                        {% if product.title == 'Stronger Immunity Tea Box' %}
                          {% assign product_herbs = herbs[0] | split: ", " %}
                        {% elsif product.title == 'Tummy Tamer Tea Box' %}
                          {% assign product_herbs = herbs[2] | split: ", " %}
                        {% elsif product.title == 'Night Time Herbal Tea Box' %}
                          {% assign product_herbs = herbs[1] | split: ", " %}
                        {% endif %}
                        <img class="herbs_image lazyload"
                            data-src="{{ img_url }}"
                            data-widths="[180, 360, 540, 720, 900, 1080, 1296, 1512, 1728, 2048]"
                            data-aspectratio="{{ image.aspect_ratio }}"
                            data-sizes="auto"
                            data-parent-fit="cover"
                            alt="{{ image.alt | escape }}">
                         <div>{{ product_herbs[idx] }}</div>
                         {% assign idx = idx | plus: 1 %}
                        <noscript>
                          <img src="{{ image | img_url: 'master' }}" alt="{{ image.alt | escape }}" class="product__subimage">
                        </noscript>
                      </a>
                    </div>
                  {%  endunless %}
                  {%  endif %}
                {% endfor %}
              </div>
          </div>
        </div>
      </div>
    </div>
  </div>


  {% comment %}
  ------------------------------------------------------------------------------
  Product Share Buttons
  ------------------------------------------------------------------------------
 
  {% if section.settings.show_share_buttons %}
    {% if settings.share_facebook or settings.share_twitter or settings.share_pinterest %}
      <div class="product__share-wrapper product__share-wrapper--mobile medium-up--hide">
        <div class="product__share">
            {% include 'social-sharing', type: 'mobile', share_title: product.title, share_permalink: product.url, share_image: product %}
        </div>
      </div>
    {% endif %}
  {% endif %}
 {% endcomment %}
 
