                  <span class="visually-hidden">{{ 'sections.product_template.slide' | t: number: forloop.index}}</span>
                </button>
              {% endfor %}
            </div>

            <button class="product-slideshow__next btn btn--secondary btn--square" tabindex="-1" data-product-slideshow-next>
              {% include 'icon-arrow-right' %}
              <span class="visually-hidden">{{ 'general.pagination.next' | t }}</span>
            </button>
          </div>
        {% endunless %}
      </div>
    </div>
  {% endunless %}

  {% comment %}
  ------------------------------------------------------------------------------
  Product Data
  ------------------------------------------------------------------------------
  {% endcomment %}
  <script type="text/javascript">
    function changeContent(id) {
      var _id = parseInt(id) - 1;
      document.querySelectorAll('.product_included').forEach(function(e) { e.classList.remove('active') });
      document.querySelector('.product' + _id).classList.add('active');
    }
    $(function() {
      $('.review-slider').owlCarousel({
        items: 1,
        loop: true,
        center: true,
        margin: 10,
        autoplay: false,
        autoplayHoverPause: true
      });

      var ten_images_slider = undefined;
      if(window.screen.width <= 425) {
        ten_images_slider = $('.product__variants_container_sm').owlCarousel({
          items: 1,
          loop: true,
          nav: true,
          margin: 10,
          center: true,
          autoplay: true,
          autoplayHoverPause: true
        });

        // bind function to event
        ten_images_slider.on('changed.owl.carousel', function(event) {
          changeContent(event.page.index + 1);
        })
      } else {
        if (ten_images_slider) {
          ten_images_slider.off('change.owl.carousel');
        }
      }
    });
  </script>
  <script type="application/json" data-product-json>
    {{ product | json }}
  </script>
</div>

